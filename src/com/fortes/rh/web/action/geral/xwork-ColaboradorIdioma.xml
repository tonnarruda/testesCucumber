<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">

<xwork>
	<package name="colaboradorIdioma" extends="default" namespace="/colaboradorIdioma">
		<action name="list" class="com.fortes.rh.web.action.geral.ColaboradorIdiomaListAction" method="list">
			<result name="success" type="freemarker">/colaboradorIdiomaList.ftl</result>
		</action>
		<action name="delete" class="com.fortes.rh.web.action.geral.ColaboradorIdiomaListAction" method="delete">
			<result name="success" type="redirect">list.action</result>
		</action>
		<action name="prepareInsert" class="com.fortes.rh.web.action.geral.ColaboradorIdiomaEditAction" method="prepareInsert">
			<result name="success" type="freemarker">/colaboradorIdiomaEdit.ftl</result>
		</action>
		<action name="prepareUpdate" class="com.fortes.rh.web.action.geral.ColaboradorIdiomaEditAction" method="prepareUpdate">
			<result name="success" type="freemarker">/colaboradorIdiomaEdit.ftl</result>
		</action>
		<action name="insert" class="com.fortes.rh.web.action.geral.ColaboradorIdiomaEditAction" method="insert">
		    <interceptor-ref name="tokenWebStack" />
		    <result name="invalid.token" type="redirect">list.action</result>
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="freemarker">/colaboradorIdiomaEdit.ftl</result>
		</action>
		<action name="update" class="com.fortes.rh.web.action.geral.ColaboradorIdiomaEditAction" method="update">
		    <interceptor-ref name="tokenWebStack" />
		    <result name="invalid.token" type="redirect">list.action</result>
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="freemarker">/colaboradorIdiomaEdit.ftl</result>
		</action>
	</package>
</xwork>