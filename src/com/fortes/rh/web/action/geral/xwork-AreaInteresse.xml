<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">

<xwork>
	<package name="areaInteresse" extends="default" namespace="/geral/areaInteresse">
		<action name="list" class="com.fortes.rh.web.action.geral.AreaInteresseListAction" method="list">
			<interceptor-ref name="defaultListStack"/>
			<result name="success" type="freemarker">/WEB-INF/forms/geral/areaInteresseList.ftl</result>
		</action>
		<action name="delete" class="com.fortes.rh.web.action.geral.AreaInteresseListAction" method="delete">
			<interceptor-ref name="defaultDeleteStack"/>
			<result name="success" type="redirect">list.action</result>
			<result name="error" type="redirect">list.action</result>
		</action>
		<action name="prepareInsert" class="com.fortes.rh.web.action.geral.AreaInteresseEditAction" method="prepareInsert">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/areaInteresseEdit.ftl</result>
		</action>
		<action name="prepareUpdate" class="com.fortes.rh.web.action.geral.AreaInteresseEditAction" method="prepareUpdate">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/areaInteresseEdit.ftl</result>
		</action>
		<action name="insert" class="com.fortes.rh.web.action.geral.AreaInteresseEditAction" method="insert">
		    <interceptor-ref name="tokenWebStack" />
		    <result name="invalid.token" type="redirect">list.action</result>
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="freemarker">/WEB-INF/forms/geral/areaInteresseEdit.ftl</result>
		</action>
		<action name="update" class="com.fortes.rh.web.action.geral.AreaInteresseEditAction" method="update">
		    <interceptor-ref name="tokenWebStack" />
		    <result name="invalid.token" type="redirect">list.action</result>
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="freemarker">/WEB-INF/forms/geral/areaInteresseEdit.ftl</result>
		</action>
	</package>
</xwork>