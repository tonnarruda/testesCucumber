<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">

<xwork>
	<package name="documentoAnexo" extends="default" namespace="/geral/documentoAnexo">
		<action name="listCandidato" class="com.fortes.rh.web.action.geral.DocumentoAnexoListAction" method="list">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoList.ftl</result>
		</action>
		<action name="listColaborador" class="com.fortes.rh.web.action.geral.DocumentoAnexoListAction" method="list">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoList.ftl</result>
		</action>
		<action name="deleteCandidato" class="com.fortes.rh.web.action.geral.DocumentoAnexoListAction" method="deleteCandidato">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoList.ftl</result>
		</action>
		<action name="deleteColaborador" class="com.fortes.rh.web.action.geral.DocumentoAnexoListAction" method="deleteColaborador">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoList.ftl</result>
		</action>
		<action name="prepareInsertCandidato" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="prepareInsertCandidato">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="prepareInsertColaborador" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="prepareInsertColaborador">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="prepareUpdateCandidato" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="prepareUpdateCandidato">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="prepareUpdateColaborador" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="prepareUpdateColaborador">
			<result name="success" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="insertCandidato" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="insertCandidato">
			<result name="success" type="redirect"><![CDATA[listCandidato.action?documentoAnexo.origem=${origemTmp}&documentoAnexo.origemId=${origemIdTmp}&nome=${nomeTmp}&solicitacaoId=${solicitacaoId}]]></result>
			<result name="input" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="insertColaborador" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="insertColaborador">
			<result name="success" type="redirect"><![CDATA[listColaborador.action?documentoAnexo.origem=${origemTmp}&documentoAnexo.origemId=${origemIdTmp}&nome=${nomeTmp}]]></result>
			<result name="input" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="updateCandidato" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="updateCandidato">
			<result name="success" type="redirect"><![CDATA[listCandidato.action?documentoAnexo.origem=${origemTmp}&documentoAnexo.origemId=${origemIdTmp}&nome=${nomeTmp}&solicitacaoId=${solicitacaoId}]]></result>
			<result name="successColaborador" type="redirect"><![CDATA[listColaborador.action?documentoAnexo.origem=${origemTmp}&documentoAnexo.origemId=${origemIdTmp}&nome=${nomeTmp}&origem=${origemDocumento}&origemId=${origemIdDocumento}]]></result>
			<result name="input" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="updateColaborador" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="updateColaborador">
			<result name="success" type="redirect"><![CDATA[listColaborador.action?documentoAnexo.origem=${origemTmp}&documentoAnexo.origemId=${origemIdTmp}&nome=${nomeTmp}]]></result>
			<result name="input" type="freemarker">/WEB-INF/forms/geral/documentoAnexoEdit.ftl</result>
		</action>
		<action name="showDocumento" class="com.fortes.rh.web.action.geral.DocumentoAnexoEditAction" method="showDocumento" />
	</package>
</xwork>