<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.0.dtd">

<xwork>
	<package name="habilidade" extends="default" namespace="/captacao/habilidade">
		<action name="list" class="com.fortes.rh.web.action.captacao.HabilidadeEditAction" method="list">
			<interceptor-ref name="defaultListStack"/>
			<result name="success" type="freemarker">/WEB-INF/forms/captacao/habilidadeList.ftl</result>
		</action>
		<action name="delete" class="com.fortes.rh.web.action.captacao.HabilidadeEditAction" method="delete">
			<interceptor-ref name="defaultDeleteStack"/>
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="redirect">list.action</result>
			<result name="error" type="redirect">list.action</result>
		</action>
		<action name="prepareInsert" class="com.fortes.rh.web.action.captacao.HabilidadeEditAction" method="prepareInsert">
			<result name="success" type="freemarker">/WEB-INF/forms/captacao/habilidadeEdit.ftl</result>
		</action>
		<action name="prepareUpdate" class="com.fortes.rh.web.action.captacao.HabilidadeEditAction" method="prepareUpdate">
			<result name="success" type="freemarker">/WEB-INF/forms/captacao/habilidadeEdit.ftl</result>
		</action>
		<action name="insert" class="com.fortes.rh.web.action.captacao.HabilidadeEditAction" method="insert">
			<interceptor-ref name="tokenWebStack" />
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="freemarker">/WEB-INF/forms/captacao/habilidadeEdit.ftl</result>
		</action>
		<action name="update" class="com.fortes.rh.web.action.captacao.HabilidadeEditAction" method="update">
			<interceptor-ref name="tokenWebStack" />
			<result name="success" type="redirect">list.action</result>
			<result name="input" type="freemarker">/WEB-INF/forms/captacao/habilidadeEdit.ftl</result>
		</action>
	</package>
</xwork>
