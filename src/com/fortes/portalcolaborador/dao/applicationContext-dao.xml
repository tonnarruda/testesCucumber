<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  <bean id="transacaoPCDao" class="org.springframework.aop.framework.ProxyFactoryBean">
    <property name="proxyInterfaces">
      <value>com.fortes.portalcolaborador.dao.TransacaoPCDao</value>
    </property>
    <property name="target">
      <ref bean="transacaoPCDaoTarget" />
    </property>
  </bean>
  <bean id="movimentacaoOperacaoPCDao" class="org.springframework.aop.framework.ProxyFactoryBean">
    <property name="proxyInterfaces">
      <value>com.fortes.portalcolaborador.dao.MovimentacaoOperacaoPCDao</value>
    </property>
    <property name="target">
      <ref bean="movimentacaoOperacaoPCDaoTarget" />
    </property>
  </bean>
   <bean id="colaboradorPCDao" class="org.springframework.aop.framework.ProxyFactoryBean">
    <property name="proxyInterfaces">
      <value>com.fortes.portalcolaborador.dao.ColaboradorPCDao</value>
    </property>
    <property name="target">
      <ref bean="colaboradorPCDaoTarget" />
    </property>
  </bean>
</beans>

